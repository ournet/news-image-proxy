{
  "name": "news-image-proxy",
  "version": "0.1.0",
  "description": "Ournet news image proxy",
  "main": "./lib/app.js",
  "private": true,
  "scripts": {
    "start": "node ./lib/app.js",
    "dev": "npm run tsc && node ./lib/app.js",
    "pretest": "npm run clean && npm run tsc",
    "test": "ava --no-cache --verbose  ./lib/*.test.js ./lib/**/*.test.js",
    "posttest": "npm run clean-test-code",
    "tsc": "tsc",
    "tscw": "tsc -w",
    "preversion": "npm run tsc",
    "postversion": "git push --follow-tags",
    "prepublish": "npm run tsc && npm run clean-test-code",
    "clean": "rimraf ./lib",
    "clean-test-code": "rimraf ./lib/**/*.test.js && rimraf ./lib/*.test.js",
    "watch": "npm-watch start"
  },
  "watch": {
    "start": {
      "patterns": [
        "lib"
      ],
      "extensions": "js",
      "quiet": true,
      "legacyWatch": true,
      "delay": 2000
    }
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Ournet/news-image-proxy.git"
  },
  "keywords": [
    "ournet",
    "news-image-proxy",
    "proxy"
  ],
  "author": "Dumitru Cantea",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Ournet/news-image-proxy/issues"
  },
  "homepage": "https://github.com/Ournet/news-image-proxy#readme",
  "devDependencies": {
    "@types/boom": "^7.2.1",
    "@types/express": "^4.16.0",
    "@types/node": "^10.12.7",
    "@types/sharp": "^0.22.0",
    "npm-watch": "^0.4.0",
    "rimraf": "^2.6.2",
    "typescript": "^3.7.5"
  },
  "dependencies": {
    "@ournet/api-client": "^0.3.x",
    "@ournet/images-domain": "^0.1.4",
    "boom": "^7.3.0",
    "cors": "^2.8.5",
    "debug": "^4.1.0",
    "dotenv": "^6.1.0",
    "entipic.url": "^0.1.0",
    "express": "^4.17.3",
    "is-stream": "^1.1.0",
    "moment": "^2.24.0",
    "ournet.links": "^0.4.12",
    "sharp": "^0.28.3",
    "slang": "^0.3.0",
    "wkhtmltoimage": "^0.1.5"
  }
}
